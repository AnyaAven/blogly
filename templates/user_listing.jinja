{% extends 'base.jinja' %}
{% block content %}

    <!-- Test comment for user_listing -->
    <h1>Users</h1>

    <ul>
      {% for user in users %}

        <li><a href="/users/{{ user.id }}">{{ user.get_full_name() }}</a></li>
      {% endfor %}
    </ul>


    <a href="/users/new" class="btn btn-primary btn-lg w-100">
        Add user
    </a>


{% endblock %}


